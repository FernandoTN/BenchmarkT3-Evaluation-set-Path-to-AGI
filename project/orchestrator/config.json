  "quality_thresholds": {
    "min_crit_score": 5.0,
    "target_crit_score": 7.0,
    "max_similarity": 0.75,
    "structure_pass_rate": 0.95,
    "dag_validity_rate": 0.98,
    "description": {
      "min_crit_score": "Minimum acceptable CRIT rubric score (0-10 scale)",
      "target_crit_score": "Target CRIT score for high-quality cases",
      "max_similarity": "Maximum cosine similarity to existing cases",
      "structure_pass_rate": "Required pass rate for structural validation",
      "dag_validity_rate": "Required pass rate for DAG acyclicity checks"
    }
  },

  "revision_settings": {
    "max_revision_cycles": 3,
    "severity_levels": {
      "CRITICAL": {
        "priority": 1,
        "must_fix": true,
        "description": "Blocks case acceptance - structural or logical errors"
      },
      "HIGH": {
        "priority": 2,
        "must_fix": true,
        "description": "Significant quality issues affecting validity"
      },
      "MEDIUM": {
        "priority": 3,
        "must_fix": false,
        "description": "Quality improvements recommended"
      },
      "LOW": {
        "priority": 4,
        "must_fix": false,
        "description": "Minor enhancements or stylistic suggestions"
      }
    },
    "escalation_rules": {
      "after_cycle_2": "Escalate to manual review if CRITICAL issues persist",
      "after_cycle_3": "Mark as failed, log for analysis"
    }
  },

  "l3_ground_truth_distribution": {
    "VALID": {
      "target_percentage": 30,
      "description": "Counterfactual claim is valid given the structural model"
    },
    "INVALID": {
      "target_percentage": 20,
      "description": "Counterfactual claim is invalid given the structural model"
    },
    "CONDITIONAL": {
      "target_percentage": 50,
      "description": "Validity depends on additional assumptions or context"
    }
